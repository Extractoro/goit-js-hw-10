{{#each this}}
    <article class="card">
        <div class="wrapper">
            <img src={{flags.svg}} alt={{name.common}} width='120' height='100' >
            <h2 class="title">{{name.official}}</h2>
        </div>
        <p class="item"><b>Capital: </b>{{capital}}</p>
        <p class="item"><b>Population: </b>{{population}}</p>
        <p class="item"><b>Languages:</b>
        <span class='value'>{{#each languages}}{{#if @last}}{{this}}{{else}}{{this}},{{/if}}
        {{/each}}</span></p>
    </article>
{{/each}}